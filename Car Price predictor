      "cell_type": "code",
      "source": [
        "# dropping\n",
        "df = df.dropna()"
      ],
      "metadata": {
        "id": "JgNvrUmj8mpG"
      },
      "execution_count": 21,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def extract_units(strings):\n",
        "    units = set()\n",
        "    for value in strings:\n",
        "        words = value.split()\n",
        "        for i in range(len(words) - 1):\n",
        "            unit = words[i + 1]\n",
        "            if unit in units:\n",
        "                continue\n",
        "            else:\n",
        "                units.add(unit)\n",
        "    return units"
      ],
      "metadata": {
        "id": "xBZzJ6PV80Wy"
      },
      "execution_count": 22,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# finding unique units\n",
        "mileage_units = extract_units(df['Mileage'])\n",
        "print(f'Unique Mileage units: {mileage_units}')\n",
        "\n",
        "mileage_units = extract_units(df['Engine'])\n",
        "print(f'Unique Engine volume units: {mileage_units}')\n",
        "\n",
        "mileage_units = extract_units(df['Power'])\n",
        "print(f'Unique Power units: {mileage_units}')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
